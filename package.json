{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@mantine/core": "^4.1.3",
    "@mantine/hooks": "^4.1.3",
    "@mantine/next": "^4.1.3",
    "@mantine/rte": "^4.1.3",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "core-js": "3",
    "express": "^4.17.3",
    "highlight.js": "^11.5.1",
    "http-errors": "^2.0.0",
    "http-status": "^1.5.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.9",
    "morgan": "^1.10.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "regenerator-runtime": "^0.13.9",
    "tabler-icons-react": "^1.45.0"
  },
  "scripts": {
    "dev:client": "webpack serve --config ./config/webpack.config.js",
    "build:client": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.js",
    "dev:server": "nodemon server/app.js"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      ".git",
      "node_modules/**/node_modules"
    ],
    "verbose": true,
    "watch": [
      "server/*"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js,json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "semi": true,
    "tabWidth": 2,
    "singleQuote": true
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.5",
    "@svgr/webpack": "^6.2.1",
    "babel-loader": "^8.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.12",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.4.3",
    "react-refresh": "^0.12.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  }
